<title>서비스 이용 약관</title>
<link rel="stylesheet" type="text/css" href="/css/font.css">
<link rel="stylesheet" type="text/css" href="/css/member_Join.css">
<script src="/js/jquery.min.js"></script>
<script src="/js/common.js"></script>

<script type="text/javascript">

	$(document).ready(function(){
	
		$("#checkAll").change(function () {
		    $("input:checkbox").prop('checked', $(this).prop("checked"));
		});
		
	});
	
	function fn_memberJoin(menuType) {		
		var cnt = 0;
		var funnel = <%= request.getParameter("funnel")%>;
		var funnelYear = <%= request.getParameter("funnelYear")%>;
		if($("#approval1").prop("checked")) cnt++;
		if($("#approval2").prop("checked")) cnt++;
		if($("#approval3").prop("checked")) cnt++;
		if($("#approval4").prop("checked")) cnt++;		
		var funnelUrl = "";
		if(cnt>=4) {
			if(funnel == null || funnel == ''){
				funnel = 100;
			}
			if(funnel == "200"){
				funnelUrl = "&funnelYear="+funnelYear;
			}
			location.href="/menu/goMemberJoin2.do?menuType="+menuType+"&funnel="+funnel+funnelUrl;
		} else {
			alert("약관에 동의해주시기 바랍니다.");
			return;
		}
		
	}
	
</script>

</head>
